{
    "file": {
        "css": "css/style.min.css"
    },
    "page": {
        "uikit": {
            "title": "UI Kit",
            "projectname": "Boilerplate Standard",
            "flexgrid": {
                "show": true,
                "gridcontainer": "1200px",
                "gridcolumns": 12,
                "gridcolors": "#d9d9d9"
            },
            "floatgrid": {
                "show": false,
                "gridcontainer": "1200px",
                "gridcolumns": 12,
                "rowclass": "row",
                "columnclass": "column",
                "gridcolors": "#d9d9d9"
            },
            "svgicons": {
                "spritepath": "images/symbol/sprite.svg",
                "icons": ["facebook","google-plus","instagram","twitter","youtube"],
                "size": "medium",
                "color": "#8c8c8c",
                "switcher": {
                    "show": true,
                    "bordercolor": "#e5e5e5",
                    "shadowcolor": "#d9d9d9"
                }
            }
        }
    }
}
