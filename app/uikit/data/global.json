{
    "renderfile": {
        "css": "css/renderkit.min.css"
    },
    "uikit": {
        "title": "UI Kit",
        "projectname": "RenderKit",
        "github": {
            "show": true
        },
        "js": {
            "whatinput": true,
            "svg4everybody": true
        }
    },
    "breakpoint": {
        "small": 0,
        "medium": 640,
        "large": 1024,
        "xlarge": 1200,
        "xxlarge": 1440
    },
    "fonts": {
        "show": true
    },
    "typography": {
        "show": true
    },
    "palette": {
        "show": true
    },
    "xygrid": {
        "show": true,
        "gridcolumns": 12,
        "gridcontainer": "1200px"
    },
    "flexbox": {
        "show": true
    },
    "floatgrid": {
        "show": false,
        "gridcolumns": 12,
        "gridcontainer": "1200px",
        "rowclass": "row",
        "columnclass": "column"
    },
    "form": {
        "show": true,
        "rowclass": "form-row",
        "labelclass": "form-label",
        "errorclass": "error",
        "selectclass": "default"
    },
    "svg": {
        "show": true,
        "spritepath": "images/symbol/sprite.svg",
        "icons": ["facebook","google-plus","instagram","twitter","youtube"],
        "collection": true,
        "examplefile": "images/symbol/sprite.symbol.html",
        "truetone": true
    }
}
