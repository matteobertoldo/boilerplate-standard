{
    "renderfile": {
        "css": "css/style.min.css"
    },
    "uikit": {
        "title": "UI Kit",
        "projectname": "Boilerplate Standard"
    },
    "breakpoint": {
        "small": 0,
        "medium": 640,
        "large": 1024,
        "xlarge": 1200,
        "xxlarge": 1440
    },
    "xygrid": {
        "show": true,
        "gridcolumns": 12,
        "gridcontainer": "1200px"
    },
    "floatgrid": {
        "show": false,
        "gridcolumns": 12,
        "gridcontainer": "1200px",
        "rowclass": "row",
        "columnclass": "column"
    },
    "form": {
        "show": true,
        "selectclass": "default"
    },
    "svg": {
        "show": true,
        "spritepath": "images/symbol/sprite.svg",
        "icons": ["facebook","google-plus","instagram","twitter","youtube"],
        "collection": true,
        "examplefile": "images/symbol/sprite.symbol.html",
        "truetone": true
    }
}
