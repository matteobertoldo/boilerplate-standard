{% set fillcolor = page.uikit.svgicons.color %}
{% set bordercolor = page.uikit.svgicons.switcher.bordercolor %}
{% set shadowcolor = page.uikit.svgicons.switcher.shadowcolor %}
{% set socialfill = {
    facebook: '#3b5999',
    twitter: '#55acee',
    linkedin: '#0077B5',
    youtube: '#cd201f',
    pinterest: '#bd081c',
    "google-plus": '#dd4b39',
    "gplus": '#dd4b39'
} %}
<style type="text/css">
#renderkit-body #uikit-svg-list {
    display: block;
    width: 100%;
    max-width: 1400px;
    list-style-type: none;
    letter-spacing: normal;
    line-height: normal;
    text-align: center;
    margin: 40px auto 0;
    padding: 0;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

#renderkit-body #uikit-svg-list li {
    display: inline-block;
    position: relative;
    vertical-align: middle;
    width: 120px;
    height: auto;
    text-align: center;
    letter-spacing: normal;
    margin: 0;
    padding: 16px 16px 25px;
}

#renderkit-body #uikit-svg-list li svg.visuallyhidden {
    position: absolute;
    top: 0;
    left: 0;
    width: 1px;
    height: 1px;
    clip: rect(1px 1px 1px 1px);
    margin: -1px;
    padding: 0;
}

#renderkit-body #uikit-svg-list li svg {
    display: inline-block;
    vertical-align: middle;
    position: relative;
    top: -.0625em;
    width: 45px;
    height: 100%;
    fill: #666;
}

{% for svgicons, fill in socialfill %}
    #renderkit-body #uikit-svg-list.true-tone li svg.{{svgicons}} {
        fill: {{fill}};
    }
{% endfor %}

#renderkit-body #uikit-svg-list.true-tone li svg.instagram {
    fill: url(#instagram-gradient);
}

#renderkit-body #uikit-switch-render  {
    margin-top: 5px;
}
</style>
